{
    "name": "jeylabs/php-witai",
    "type": "library",
    "description": "PHP Library for Wit.ai API with Laravel & Lumen Support out of the box!",
    "keywords": [
        "jeylabs",
        "php-witai",
        "wit",
        "wit.ai",
        "witai",
        "php-wit",
        "laravel",
        "lumen",
        "laravel wit",
        "lumen wit"
    ],
    "homepage": "https://github.com/jeylabs/php-witai",
    "license": "MIT",
    "authors": [
        {
            "name": "jeylabs",
            "email": "suresh.ceymplon@jeylabs.com",
            "homepage": "https://jeylabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "guzzlehttp/guzzle": "~6.2",
        "illuminate/support": "5.3.*",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Irazasyed\\Wit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Irazasyed\\Wit\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
